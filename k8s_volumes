---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: mydep1
spec:
  replicas: 1
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      name: mytemp1
      labels:
        app: nginx
    spec:
      containers:
        - name: con1
          image: nginx
          volumeMounts:
            - mountPath: /opt/srinu
              name: myvol1

        - name: con2
          image: httpd
          command: ['/bin/bash', '-c', 'while true; do echo happy pongal; sleep 10; done']
          volumeMounts:
            - mountPath: /opt/siri
              name: myvol1

      volumes:
        - name: myvol1
          hostPath:
            path: /etc/anu
